e.\r\n*/\r\n\r\npragma solidity 0.5.7;\r\n\r\n\r\n\r\n/**\r\n * @title ConstantPriceOracle\r\n * @author Set Protocol\r\n *\r\n * Oracle built to adhere to IOracle interface the returns a constant price. Intended\r\n * for stablecoin use.\r\n */\r\ncontract ConstantPriceOracle is\r\n    IOracle\r\n{\r\n    /* ============ State Variables ============ */\r\n    uint256 public stablePrice;\r\n\r\n    /* ============ Constructor ============ */\r\n    /*\r\n     * Set price oracle is made to return\r\n     *\r\n     * @param  _oracleAddress    The address of oracle being proxied\r\n     */\r\n    constructor(\r\n        uint256 _stablePrice\r\n    )\r\n        public\r\n    {\r\n        stablePrice = _stablePrice;\r\n    }\r\n\r\n    /**\r\n     * Returns the stable price\r\n     *\r\n     * @return  Stable price of asset represented in uint256\r\n     */\r\n    function read()\r\n        external\r\n        view\r\n        returns (uint256)\r\n    {\r\n        return stablePrice;\r\n    }\r\n}","ABI":"[{\u0022constant\u0022:true,\u0022inputs\u0022:[],\u0022name\u0022:\u0022read\u0022,\u0022outputs\u0022:[{\u0022name\u0022:\u0022\u0022,\u0022type\u0022:\u0022uint256\u0022}],\u0022payable\u0022:false,\u0022stateMutability\u0022:\u0022view\u0022,\u0022type\u0022:\u0022function\u0022},{\u0022constant\u0022:true,\u0022inputs\u0022:[],\u0022name\u0022:\u0022stablePrice\u0022,\u0022outputs\u0022:[{\u0022name\u0022:\u0022\u0022,\u0022type\u0022:\u0022uint256\u0022}],\u0022payable\u0022:false,\u0022stateMutability\u0022:\u0022view\u0022,\u0022type\u0022:\u0022function\u0022},{\u0022inputs\u0022:[{\u0022name\u0022:\u0022_stablePrice\u0022,\u0022type\u0022:\u0022uint256\u0022}],\u0022payable\u0022:false,\u0022stateMutability\u0022:\u0022nonpayable\u0022,\u0022type\u0022:\u0022constructor\u0022}]","ContractName":"ConstantPriceOracle","CompilerVersion":"v0.5.7\u002Bcommit.6da8b019","OptimizationUsed":"1","Runs":"200","ConstructorArguments":"0000000000000000000000000000000000000000